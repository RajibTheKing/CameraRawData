<Window x:Class="CameraRawData.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="768" Width="1024">
    <Grid>
        <Button Content="Stop Camera" HorizontalAlignment="Left" Margin="31,310,0,0" VerticalAlignment="Top" Width="128" RenderTransformOrigin="-0.347,0.5" Height="24" Click="Button_Click"/>
        <Button Content="Start Camera" HorizontalAlignment="Left" Margin="31,188,0,0" VerticalAlignment="Top" Width="128" Click="Button_Click_1" RenderTransformOrigin="1.136,0.309"/>
        <Image Source="{Binding Path=MyImageSource}" Width="640" Height="480" Margin="300,134,76,124"/>
        <Image Source="Images/logo.png" HorizontalAlignment="Left" Height="100" Margin="10,10,0,0" VerticalAlignment="Top" Width="355"/>
    </Grid>
</Window>
